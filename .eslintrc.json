{
	"extends": [
		"prettier"
	],
	"plugins": [
		"@typescript-eslint",
		"prettier"
	],
	"env": {
		"node": true,
		"jest": true,
		"browser": true,
		"es6": true
	},
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"sourceType": "module"
	},
	"rules": {
		"prettier/prettier": "error",
		"@typescript-eslint/no-unused-vars": [
			"error",
			{
				"varsIgnorePattern": "^_",
				"args": "none"
			}
		],
		"no-var": "error",
		"prefer-const": "error",
		"max-params": [
			"error",
			9
		]
	}
}
